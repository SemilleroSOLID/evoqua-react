{
  "version": "0.1.0",
  "license": "ISC",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@evoqua/react",
  "author": "jlopezg8",
  "module": "dist/react.esm.js",
  "size-limit": [
    {
      "path": "dist/react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@evoqua/types": "github:jlopezg8/evoqua-types#de327af2083e4402f467c442f23152819fc4fb2f",
    "@size-limit/preset-small-lib": "^5.0.5",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-plotly.js": "^2.5.0",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "plotly.js": "^2.5.1",
    "react-plotly.js": "^2.5.1",
    "react-select": "^5.0.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "description": "Visualizador de la evolución de métricas de un software basado en tubos de kiviat",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlopezg8/evoqua-react.git"
  },
  "bugs": {
    "url": "https://github.com/jlopezg8/evoqua-react/issues"
  },
  "homepage": "https://github.com/jlopezg8/evoqua-react#readme"
}
